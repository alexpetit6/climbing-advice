<%- include('../partials/header') %>

<% posts.forEach(function(p) { %>
    <div class="posts">
        <a href="/posts/<%= p._id %>">
            <h4><span class="light">User:</span> <%= p.userName %></h4>
            <h1><%= p.title %></h1>
            <iframe width="560" height="315" src="https://www.youtube.com/embed/<%= p.video %>" frameborder="0"></iframe>
        </a>
    </div>
<% }) %>

<%- include('../partials/footer') %>